<head>
  <title>Homespree | The smart way to get home improvement estimates</title>

  <%= render "partials/header_extras" %>

  <meta name="description" content="Homespree is the smart way to get home improvement estimates.">
  <meta name="viewport" content="width=device-width">

  <%= stylesheet_link_tag "application" %>
  <%= csrf_meta_tags %>

</head>

<body class="desktop">


<%= render "partials/flash", flash: flash %>
<%= render "partials/header" %>

<div id="content">
  <%= content_for?(:content) ? yield(:content) : yield %>
</div>

<%= render "partials/footer" %>
<%= render "partials/footer_extras" %>

<%= javascript_include_tag 'application' %>

</body>
