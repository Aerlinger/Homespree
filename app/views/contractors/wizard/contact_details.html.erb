<% content_for :registration_page do %>

  <%= provide :title, "Your contact details" %>

  <%= render layout: 'form' do |f| %>

    <%= f.input :first_name %>
    <%= f.input :last_name %>

    <%= f.input :specialties %>

    <%= f.input :mobile_number %>
    <%= f.input :office_number %>

    <%= f.input :website %>

  <% end %>

<% end %>