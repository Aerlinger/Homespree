<% content_for :registration_page do %>
  <%= provide :title, "Promote yourself" %>

  <%= render layout: 'form' do |f| %>
    <%= f.input :slogan %>
    <%= f.input :description %>
    <%= f.input :photo_filename %>
    <%= f.input :website %>
    <%= f.input :facebook %>
    <%= f.input :twitter %>
  <% end %>
<% end %>